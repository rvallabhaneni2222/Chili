                 http://jboss.com/products/seam/security http://jboss.com/products/seam/security-2.2.xsd
                 http://jboss.com/products/seam/theme http://jboss.com/products/seam/theme-2.2.xsd
                 http://jboss.com/products/seam/cache http://jboss.com/products/seam/cache-2.2.xsd
                 http://jboss.com/products/seam/web http://jboss.com/products/seam/web-2.2.xsd
                 http://jboss.com/products/seam/components http://jboss.com/products/seam/components-2.2.xsd
                 http://jboss.com/products/seam/drools http://jboss.com/products/seam/drools-2.2.xsd">

	<security:jpa-identity-store
		user-class="${$groupID}.entity.security.YUser"
		role-class="${$groupID}.entity.security.YRole" 
		entity-manager="#{em}" />
	
		
		<persistence:managed-persistence-context name="em"
		auto-create="true" persistence-unit-jndi-name="java:/${$projectName}"/>
</components>


