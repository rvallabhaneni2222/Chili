	protected Button delete;
	
	@Override
	public void onCreate(Bundle savedInstanceState) {
		super.onCreate(savedInstanceState);
		requestWindowFeature(Window.FEATURE_CUSTOM_TITLE);
		setContentView(R.layout.${lower-case($class/@name)}_read);
		customizeTitle();
		done = (Button) findViewById(R.id.read_done);
		done.setOnClickListener(this);
		update = (Button) findViewById(R.id.read_update);
		update.setOnClickListener(this);
		delete = (Button) findViewById(R.id.read_delete);
		delete.setOnClickListener(this);
	}
	
	public void customizeTitle(){
		getWindow().setFeatureInt(Window.FEATURE_CUSTOM_TITLE, R.layout.${lower-case($projectName)}_title_bar);
	     TextView mytitletext = (TextView) findViewById(R.id.${lower-case($projectName)}_title_text);
	     mytitletext.setText("Read ${$class/@name} Panel");	
	}
	
	public void load(String result) throws JSONException{
			${lower-case($class/@name)} = (JSONObject) new JSONObject(result).get("${lower-case($class/@name)}");
	<%-- process parent class level 2 attributes--%>
	<c:if test="not($class/@name = $parentClass2/@name)">
				<c:iterate select="$parentClass2/Classifier.feature/Attribute" var="attribute">
						ViewTextField ${lower-case($attribute/@name)}_textview = (ViewTextField) this.findViewById(R.id.${$class/@name}_read_${$attribute/@name});
						${lower-case($attribute/@name)}_textview.setLabel("${upper-case($attribute/@name)}");
						if(${lower-case($class/@name)}.has("${lowercaseFirst($attribute/@name)}"))
						${lower-case($attribute/@name)}_textview.setValue(${lower-case($class/@name)}.get("${lowercaseFirst($attribute/@name)}").toString());
