	public void onClick(View arg0) {
		if (arg0 == done) {
			finish();
		}
		if (arg0 == update) {
			Intent intent = new Intent(this, Update${$class/@name}.class);
			intent.putExtra("entity", ${lower-case($class/@name)}.toString());
			startActivity(intent);
		}
		if (arg0 == delete) {
			Toast.makeText(getApplicationContext(), "aaa", Toast.LENGTH_SHORT);
		}
	}
	
	public String getReadURL() {
	Long id = getIntent().getLongExtra("id", new Long(1));
	return ${camelCase($projectName)}Welcome.baseURL + "${lower-case($class/@name)}/read/" + id;
	}
	
	@Override
	public void onStart() {
		super.onStart();
		Log.d("info.yalamacnhili.android", "in start");
		new AsyncHttpGet(this) {
			@Override
			protected void onResponse(String result) {
			try{
				load(result);
				}
				catch(JSONException e){
					e.printStackTrace();
					//TODO 
				}
			}
